{
    "model_name": "fpua",


    "inputs": "rgb",
    "output": "s",

    "dataset": "UATD",
    "root": "/home/wangyike/UATD",
    "n_classes": 11,
    "id_unlabel": -1,
    "brightness": 0.5,
    "contrast": 0.5,
    "saturation": 0.5,
    "p": 0.5,
    "scales_range": "0.5 2.0",
    "crop_size": "480 320",
    "eval_scales": "0.5 0.75 1.0 1.25 1.5 1.75",
    "eval_flip": "true",


    "ims_per_gpu": 4,
    "num_workers": 8,

    "optim": "sgd",
    "lr_start": 0.01,
    "momentum": 0.9,
    "weight_decay": 5e-4,
    "lr_power": 0.9,
    "epochs": 100,

    "loss": "crossentropy",
    "class_weight": "enet"
}


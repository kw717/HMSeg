{
    "model_name": "HMSeg",

    "inputs": "rgb",
    "output": "m",

    "dataset": "mdd",
    "root": "/home/wangyike/watertank-segmentation",
    "n_classes": 12,
    "id_unlabel": -1,
    "brightness": 0.5,
    "contrast": 0.5,
    "saturation": 0.5,
    "p": 0.5,
    "scales_range": "0.5 2.0",
    "crop_size": "480 320",
    "eval_scales": "0.5 0.75 1.0 1.25 1.5 1.75",
    "eval_flip": "true",


    "ims_per_gpu": 16,
    "num_workers": 8,

    "optim": "ranger",
    "lr_start": 0.003,
    "momentum": 0.9,
    "weight_decay": 5e-4,
    "lr_power": 0.9,
    "epochs": 50,
    "loss1": 1,
    "L1": "ce",
    "loss2": 1.5,
    "L2": "ls",
    "loss3": 0.1,
    "L3": "ce",
    "dn": 0.1,
    "blur": "median",

    "loss": "crossentropy",
    "class_weight": "enet"
}

